void F(int m, int n)
{
    if (m != n)
    {
        if (m > n)
        {
            F(m - 1, n);
            System.Console.Write(m + " ");
        }
        else
        {

            F(m, n - 1);
            System.Console.Write(m + " ");

        }

    }
    else
    {
        System.Console.Write(m);
    }
}

F(1, 9);
